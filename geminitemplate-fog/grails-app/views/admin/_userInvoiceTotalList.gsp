<div class="row">
  <div id="main-stats">
    <div class="row stats-row">
      <div class="col-md-4 stat">
        <div class="data" id="countLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup id="dailyUsd" class="col-md-2"></sup>
               <span  class="col-md-6" id="daily">0</span>
            </div> 
          </div>
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"><g:message code="stat.usageCost"/></div>
          </div>            
          <div id="dailyDate" class="col-md-11 dbdates usagePeriod"></div>
        </div>  
      </div>
      <div class="col-md-4 stat">                
        <div class="data" id="dailyLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup class="col-md-2" id="monthlyUsd"></sup>
               <span  class="col-md-6" id="monthly">0</span>
            </div>
          </div> 
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"><g:message code="stat.draftInvoiceCost"/></div>
          </div>   
          <div class="col-md-11 dbdates usagePeriod" id="monthlyDate"></div>
        </div>       
      </div>
      <div class="col-md-4 stat">
        <div class="data" id="monthlyLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup class="col-md-2" id="preMonthUsd"></sup>
               <span  class="col-md-6" id="preMonth">0</span>                
            </div>
          </div>                    
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"><g:message code="stat.finalInvoiceCost"/></div>
          </div> 
          <div class="col-md-11 dbdates usagePeriod" id="preMonthDate"></div>
        </div>                   
      </div>      
    </div>
  </div>
</div>