<div class="row">
  <div id="main-stats">
    <div class="row stats-row">
      <div class="col-md-6 stat">
        <div class="data" id="countLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup class="col-md-2" id="recAmtCurrency"></sup>
               <span  class="col-md-6" id="recAmt">0</span>              
            </div>
          </div>
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"> <g:message code="stat.recurringItemCost"/></div>
          </div>
          <div class="col-md-11 PaymentPeriod dbdates" id="recDate" ></div> 
        </div>                                 
      </div>                   
      <div class="col-md-6 stat">
        <div class="data" id="dailyLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup id="cusAmtCurrency" class="col-md-2"></sup>
               <span  id="cusAmt" class="col-md-6">0</span>                              
            </div>
          </div>
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags">  <g:message code="stat.customItemCost"/></div>
          </div>
          <div class="col-md-11 PaymentPeriod dbdates" id="cusDate" ></div>                
        </div>          
      </div>   
    </div>
  </div>
</div>