<div class="row">
  <div id="main-stats">
    <div class="row stats-row">
      <div class="col-md-4 stat">
        <div class="data" id="countLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup class="col-md-2"  id="dailycurrency"></sup>      
               <span  class="col-md-6"  id="daily">0</span>                      
            </div>  
          </div>
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"><g:message code="stat.todayPayment"/></div>
          </div>
          <div class="col-md-11 PaymentPeriod dbdates" id="dailyDate" ></div>                                             
        </div>           
      </div>
      <div class="col-md-4 stat">
        <div class="data" id="dailyLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup id="monthlyCurrency" class="col-md-2"></sup>
               <span   id="monthly" class="col-md-6">0</span>              
            </div>
          </div>
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"><g:message code="stat.currentMonthpayment"/></div>
          </div>
          <div class="col-md-11 PaymentPeriod dbdates" id="monthlyDate" ></div>                          
        </div>
      </div>
      <div class="col-md-4 stat">
        <div class="data" id="monthlyLabel">
          <div class="col-md-7 price_info_value">
            <div class="number col-md-12">
              <sup id="preMonthCurrency"  class="col-md-2"></sup>
               <span  id="preMonth"  class="col-md-6">0</span>              
            </div>
          </div>
          <div class="col-md-5 price_info_details">
            <div class="col-md-12 info_tags"><g:message code="stat.thisYearPayment"/></div>
          </div>
          <div class="col-md-11 PaymentPeriod dbdates" id="preMonthDate" ></div>
        </div>              
      </div>      
    </div>
  </div>
</div>